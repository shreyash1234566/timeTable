{
  "version": 2,
  "builds": [
    {
      "src": "api/progress.js",
      "use": "@vercel/node"
    },
    {
      "src": "*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/progress.js"
    },
    {
      "src": "/login.html",
      "dest": "/login.html"
    },
    {
      "src": "/progress.html",
      "dest": "/progress.html"
    },
    {
      "src": "/roadmap.html",
      "dest": "/roadmap.html"
    },
    {
      "src": "/timetable.html",
      "dest": "/timetable.html"
    },
    {
      "src": "/",
      "dest": "/login.html"
    },
    {
      "src": "/(.*)",
      "dest": "/login.html"
    }
  ],
  "env": {
    "DATA_DIR": "/tmp"
  }
}
